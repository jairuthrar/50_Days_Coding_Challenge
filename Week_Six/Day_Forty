def findElement( arr, n):
    for i in range(1,n-1):
        if min(arr[i+1:]) >= arr[i] and max(arr[:i]) <= arr[i]:           
            return arr[i]
    return -1        
    
#{ 
 # Driver Code Starts
#Initial Template for Python 3

def main():

    T = int(input())

    while(T > 0):
        n = int(input())
        a = [int(x) for x in input().strip().split()]
        print(findElement(a, n))

        T -= 1


if __name__ == "__main__":
    main()





    
